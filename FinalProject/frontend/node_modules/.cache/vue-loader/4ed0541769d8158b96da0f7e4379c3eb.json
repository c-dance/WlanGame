{"remainingRequest":"/Users/zhin/git/c-dance/WlanGame/FinalProject/frontend/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/zhin/git/c-dance/WlanGame/FinalProject/frontend/src/components/GameRoom.vue?vue&type=script&lang=js&","dependencies":[{"path":"/Users/zhin/git/c-dance/WlanGame/FinalProject/frontend/src/components/GameRoom.vue","mtime":1600931002879},{"path":"/Users/zhin/git/c-dance/WlanGame/FinalProject/frontend/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/zhin/git/c-dance/WlanGame/FinalProject/frontend/node_modules/babel-loader/lib/index.js","mtime":499162500000},{"path":"/Users/zhin/git/c-dance/WlanGame/FinalProject/frontend/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/zhin/git/c-dance/WlanGame/FinalProject/frontend/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["GameRoom.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuHA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;;AAEA","file":"GameRoom.vue","sourceRoot":"src/components","sourcesContent":["<template>\n\n<v-container fluid fill-height aacontaine>\n<v-layout column>\n  <v-flex>\n    <v-row>\n      <v-app-bar app dark dense>\n      <v-col cols=\"3\">\n        <h2>{{roomName}}술모임 </h2>\n      </v-col>\n      <v-col cols=\"2\" >\n      <v-btn @click=\"chatMode\" >\n      <v-img\n          alt=\"Home Logo\"\n          class=\"shrink mr-2\"\n          contain\n          src=\"../assets/chat.png\"\n          transition=\"scale-transition\"\n          width=\"30\"\n        /> \n        <span class=\"mr-1\">채팅모드</span>\n      </v-btn>\n        </v-col>\n         <v-col cols=\"2\">\n      <v-btn @click=\"gameList\" >\n          <v-img\n          alt=\"Home Logo\"\n          class=\"shrink mr-2\"\n          contain\n          src=\"../assets/gambling.png\"\n          transition=\"scale-transition\"\n          width=\"30\"\n        /> \n        <span class=\"mr-1\">게임모드</span>\n      </v-btn>\n         </v-col>\n        <v-col cols=\"2\">\n      <v-btn @click=\"exitRoom\" >\n                  <v-img\n          alt=\"Home Logo\"\n          class=\"shrink mr-2\"\n          contain\n          src=\"../assets/exit.png\"\n          transition=\"scale-transition\"\n          width=\"30\"\n        /> \n        <span class=\"mr-1\">나가기</span>\n      </v-btn>\n      </v-col>\n      <v-col cols=\"3\">\n        <v-row >\n        <v-col cols=\"2\" v-for=\"(mem,index) in members\" :key=\"index\">\n        <br/>\n        <v-img alt=\"Home Logo\" class=\"shrink mr-2\" contain src=\"../assets/p1.png\" transition=\"scale-transition\" width=\"40\"/> \n        <p :style=\"{'font-size':'15px'}\">{{mem}}</p> \n        </v-col>\n        </v-row>\n      </v-col>\n      </v-app-bar>\n    </v-row>  \n  </v-flex>\n  <v-flex>\n    <v-expansion-panels dense v-if=\"selectMode\" justify=\"center\" dark light focusable accordion><!--selectMode-->\n      <v-expansion-panel v-for=\"(game,index) in gameIntro\" :key=\"index\">\n        <v-expansion-panel-header>{{game.title}}</v-expansion-panel-header>\n        <v-expansion-panel-content >\n          <v-row>\n          <v-col align-self=\"center\" cols=\"10\">\n            <div><p>{{game.intro}}</p></div>\n          </v-col>\n          <v-col align-self=\"center\" cols=\"2\">\n            <!--<router-link :to=\"{ path:'game'+i }\" append> >game start</router-link>-->\n            <v-btn v-on:click=\"startGame(index)\">start</v-btn>\n          </v-col>\n        </v-row>\n        </v-expansion-panel-content>\n      </v-expansion-panel>\n    </v-expansion-panels> \n  </v-flex>\n  <v-flex>\n    <div>\n    <v-layout row>\n      \n    <v-flex xs8><router-view \n                v-bind:gameData=\"gameData[selectedGame]\" \n                v-bind:members=\"members\"\n                v-on:changeGames=\"ChangeGames\"></router-view></v-flex><!-- game window-->\n      \n    <v-flex xs4>\n       <v-layout column wrap>\n            <v-flex>\n                <v-card  class=\"mx-auto\" min-height=\"440\">\n                  <v-list>\n                    <v-list-item v-for=\"(msg, index) in allMsg\" :key=\"index\">\n                    <v-list-item-avatar>{{msg.nickname}}</v-list-item-avatar>\n                    <v-list-item-content>\n                    <v-list-item-subtitle v-text=\"msg.msg\"></v-list-item-subtitle>\n                    </v-list-item-content>\n                    </v-list-item>\n                  </v-list>\n                </v-card>\n            </v-flex>\n            <v-flex>\n                <!-- <v-card class=\"mx-auto\" min-height=\"100\"></v-card> -->\n                <v-btn block v-on:click=\"send\" dense height=\"40\" color=\"#F39C12\" dark>message send</v-btn>\n                <v-textarea type=\"text\" v-model=\"myMsg\" @keyup.enter=\"send\" outlined rows=\"2\" min-height=\"80\" row-height = \"60\"></v-textarea>  \n            </v-flex>\n        </v-layout>\n      </v-flex>\n    </v-layout>\n    </div>\n  </v-flex>\n  <v-flex></v-flex>\n</v-layout>\n</v-container>\n\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default {\n  name: \"Gameroom\",\n    data(){\n        return{\n            room:null,\n            roomName:'',\n            selectMode : false,\n            myMsg : \"\",\n            allMsg:[],\n            gameIntro:[\n              {title:\"랜덤카드뽑기\", intro:\"셔플된 카드를 뽑습니다. 샷 카드가 나오면 원샷 당첨입니다.\"},\n              {title:\"끝말 잇기\", intro : \"다 같이 끝말 잇기를 합니다. 진 사람이 원샷 당첨입니다.\"},\n              {title:\"백종원 게임\", intro: \"음식 레시피를 말 해 보세요. 백종원 레시피에 포함된다면 원샷 당첨입니다.\"},\n              {title:\"훈민정음 게임\", intro : \"test\"}\n            ],\n            isGameOn:false,\n            selectedGame:0,\n        }\n    },\n     props: {\n            id : {\n                type: String,\n                default : ''\n            },\n            nickname :{\n              type: String,\n              default :''\n            }\n     },\n    methods:{\n\n        gameList(){\n            this.selectMode = !this.selectMode;\n        },\n        startGame(index){\n          this.selectMode = false\n          this.isGameOn = true,\n          this.selectedGame=index\n          console.log(this.members)\n          this.$router.push({name:\"game\"+index})\n        },\n        chatMode(){\n          this.selectMode = false;\n          this.isGameOn = false;\n          this.$router.push({name:\"chat\"});\n        },\n        exitRoom(){ \n            const memNum = this.room.mem.length\n            const id = this.room._id\n            const name = localStorage.getItem(\"nickname\")\n            if(memNum===1){\n              alert(\"술모임방의 마지막 멤버입니다. 술모임방이 사라집니다.\")\n              axios.put('http://localhost:3000/api/rooms/deleteRoom',{\n                id:id\n              })\n              .then(r=>{\n                console.log(\"delete room\") \n                localStorage.removeItem(\"nickname\")\n                this.$router.replace({path:\"/\"});\n              })\n              .catch(e=>{\n                console.log(\"delete fail\")\n              })\n\n            }\n            else{\n              alert(\"술모임에서 나갑니다.\");\n              axios.put('http://localhost:3000/api/rooms/deleteOne',{\n                id : id, nickname : name\n              })\n                .then(r=>{\n                console.log(r)\n                localStorage.removeItem(\"nickname\")\n                this.$router.replace({path:\"/\"}); \n                })\n                .catch(e=>{\n                console.log(e)\n                })\n            }\n        },\n        send(){\n          const roomId = this.room._id\n          const msg = this.myMsg.trim()\n          const nickname = localStorage.getItem(\"nickname\")\n          console.log(typeof nickname)\n          axios.put('http://localhost:3000/api/chat/newmsg',{\n            roomId:roomId, nickname : nickname, msg:msg\n          })\n            .then(r=>{console.log(r)})\n            .catch(e=>{console.log(e)})\n\n          },\n        ChangeGames:function(payload){\n          this.$store.commit(\"ChangeGames\",payload)\n        }\n    },\n    mounted(){\n      axios.get('http://localhost:3000/api/rooms/login/'+ this.$route.params.id )\n      .then((r) => {\n        this.room = r.data.room\n        this.roomName = r.data.room.name\n        this.$store.commit('ChangeMembers',r.data.room.mem)\n        console.log(localStorage.getItem(\"nickname\"))\n      })\n      .catch((e) => {\n        console.error(e.message)\n      })\n\n      axios.get('http://localhost:3000/api/chat/'+ this.$route.params.id )\n        .then((r)=>{\n          this.allMsg = r.data[0].messages\n          console.log(r.data[0])\n          console.log(this.allMsg[0])\n        })\n        .catch((e)=>{\n          console.log(\"allMsg failed\")\n        })\n    },\n    created(){\n\n    },\n    computed : {\n      members(){\n        return this.$store.getters.getMembers;\n      },\n      gameData(){\n        return this.$store.getters.getGames;\n      }\n      \n    }\n    \n}\n\n</script>\n"]}]}